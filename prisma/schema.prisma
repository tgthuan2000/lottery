generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Slot {
  id        String   @id @default(cuid())
  label     String
  value     String
  createdAt DateTime @default(now())

  @@index([id])
}
